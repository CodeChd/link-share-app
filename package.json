{
  "name": "link-sharing-app",
  "version": "1.0.0",
  "description": "share your links to public",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.ts",
    "server": "nodemon backend/server.ts",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \" npm run client\" ",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "import": "node backend/seeeder.ts",
    "delete": "node backend/seeeder.ts -d"
  },
  "author": "cj",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.20",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
